{
  "name": "rdf4js",
  "version": "1.0.0",
  "description": "rdf4js lib",
  "main": "lib/rdf4js.js",
  "module": "src/index.js",
  "scripts": {
    "build": "rimraf lib/ && cross-env NODE_ENV=production webpack --mode production && npm run doc",
    "build:dev": "rimraf lib/ && cross-env NODE_ENV=development webpack --mode development",
    "watch": "rimraf lib/ && cross-env NODE_ENV=development webpack --mode development --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:report": "rimraf report/ && jest --config jest-report.config.js",
    "lint": "eslint ./src",
    "lint:test": "eslint --no-eslintrc --config .eslintrc-test ./test",
    "doc": "jsdoc -c jsdoc.conf.json"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run lint"
    }
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "babel-loader": "^8.0.5",
    "cross-env": "^5.2.0",
    "eslint": "^5.16.0",
    "eslint-config-google": "^0.12.0",
    "eslint-plugin-no-only-tests": "^2.3.0",
    "husky": "^1.3.1",
    "jest": "^24.7.1",
    "jest-junit": "^6.3.0",
    "jest-when": "^2.4.0",
    "jsdoc": "^3.5.5",
    "stream-mock": "^2.0.2",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.0"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "n3": "^1.0.5",
    "qs": "^6.7.0",
    "uuid": "^3.3.2"
  }
}
