{
  "name": "rdf4js",
  "version": "1.0.0",
  "description": "rdf4js lib",
  "main": "lib/rdf4js.js",
  "module": "src/index.js",
  "scripts": {
    "build": "rimraf lib/ && cross-env NODE_ENV=production webpack --mode production && npm run doc",
    "build:dev": "rimraf lib/ && cross-env NODE_ENV=development webpack --mode development",
    "watch": "rimraf lib/ && cross-env NODE_ENV=development webpack --mode development --watch",
    "test": "jest",
    "lint": "eslint ./src",
    "doc": "jsdoc -c jsdoc.conf.json"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "babel-loader": "^8.0.5",
    "cross-env": "^5.2.0",
    "eslint": "^5.15.1",
    "eslint-config-google": "^0.12.0",
    "husky": "^1.3.1",
    "jest": "^24.5.0",
    "jsdoc": "^3.5.5",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.2.3"
  },
  "dependencies": {}
}
